{
  "name": "raise-website",
  "private": true,
  "author": "Adam Jones <domdomegg+git@gmail.com>",
  "main": "n/a",
  "scripts": {
    "postinstall": "cp -n src/env/local.template.ts src/env/local.ts && cp -n src/env/local.ts src/env/env.ts",
    "start": "npm run start:local",
    "start:local": "cp src/env/local.ts src/env/env.ts && gatsby develop --port 8000",
    "build": "npm run build:local",
    "build:local": "cp src/env/local.ts src/env/env.ts && npm run type-check && gatsby build",
    "build:dev": "cp src/env/dev.ts src/env/env.ts && npm run type-check && gatsby build",
    "build:prod": "cp src/env/prod.ts src/env/env.ts && npm run type-check && gatsby build",
    "deploy:dev": "npm run build:dev && serverless deploy --verbose",
    "deploy:prod": "npm run build:prod && serverless deploy --verbose",
    "clean": "rimraf public",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "TZ=utc jest",
    "test:watch": "TZ=utc jest --watch",
    "type-check": "tsc",
    "codeowners": "node codeowners.js"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "@reach/router": "^1.3.4",
    "@stripe/react-stripe-js": "^1.6.0",
    "@stripe/stripe-js": "^1.20.3",
    "axios": "^0.21.4",
    "axios-hooks": "^2.7.0",
    "babel-plugin-styled-components": "^1.10.1",
    "classnames": "^2.2.6",
    "eslint-config-blvd": "^1.2.1",
    "gatsby": "^2.10.0",
    "gatsby-image": "^2.2.1",
    "gatsby-plugin-canonical-urls": "^2.1.0",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-sharp": "^2.2.1",
    "gatsby-plugin-typescript": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^2.1.0",
    "gatsby-remark-images": "^3.1.0",
    "gatsby-remark-prismjs": "^3.3.0",
    "gatsby-remark-responsive-iframe": "^2.2.0",
    "gatsby-remark-smartypants": "^2.1.0",
    "gatsby-source-filesystem": "^2.1.0",
    "gatsby-transformer-json": "^2.2.0",
    "gatsby-transformer-remark": "^2.5.0",
    "gatsby-transformer-sharp": "^2.2.0",
    "jsonexport": "^3.2.0",
    "prism-themes": "^1.1.0",
    "prismjs": "^1.23.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-google-login": "^5.2.2",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^7.16.2",
    "react-timeago": "^6.2.1",
    "typescript": "^4.5.3"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.69.1",
    "@testing-library/react": "^12.1.2",
    "@types/classnames": "^2.2.8",
    "@types/jest": "^27.0.2",
    "@types/jsonexport": "^3.0.2",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-timeago": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "autoprefixer": "^10.3.4",
    "eslint": "^7.32.0",
    "gatsby-plugin-postcss": "^4.13.0",
    "jest": "^27.3.1",
    "postcss": "^8.3.6",
    "rimraf": "^2.6.3",
    "serverless": "^2.69.1",
    "serverless-s3-sync": "^1.17.3",
    "tailwindcss": "^2.2.15",
    "ts-jest": "^27.0.7"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  }
}
