{
  "name": "raise-website",
  "private": true,
  "author": "Adam Jones <domdomegg+git@gmail.com>",
  "main": "n/a",
  "scripts": {
    "postinstall": "cp -n src/env/local.template.ts src/env/local.ts && cp -n src/env/local.ts src/env/env.ts",
    "start": "npm run start:local",
    "start:local": "cp src/env/local.ts src/env/env.ts && gatsby develop --port 8000",
    "build": "npm run build:local",
    "build:local": "cp src/env/local.ts src/env/env.ts && npm run type-check && gatsby build",
    "build:dev": "cp src/env/dev.ts src/env/env.ts && npm run type-check && gatsby build",
    "build:prod": "cp src/env/prod.ts src/env/env.ts && npm run type-check && gatsby build",
    "deploy:dev": "npm run build:dev && serverless deploy --verbose",
    "deploy:prod": "npm run build:prod && serverless deploy --verbose",
    "clean": "rimraf public",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "TZ=utc jest",
    "test:watch": "TZ=utc jest --watch",
    "type-check": "tsc",
    "codeowners": "node codeowners.js"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.4",
    "@reach/dialog": "^0.16.2",
    "@reach/disclosure": "^0.16.2",
    "@reach/listbox": "^0.16.2",
    "@reach/router": "^1.3.4",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.22.0",
    "axios": "^0.24.0",
    "axios-hooks": "^3.0.0",
    "babel-plugin-styled-components": "^2.0.2",
    "classnames": "^2.3.1",
    "eslint-config-blvd": "^1.2.1",
    "gatsby": "^4.4.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-canonical-urls": "^4.4.0",
    "gatsby-plugin-image": "^2.4.0",
    "gatsby-plugin-react-helmet": "^5.4.0",
    "gatsby-plugin-sharp": "^4.4.0",
    "gatsby-plugin-typescript": "^4.4.0",
    "gatsby-remark-copy-linked-files": "^5.4.0",
    "gatsby-remark-images": "^6.4.0",
    "gatsby-remark-prismjs": "^6.4.0",
    "gatsby-remark-responsive-iframe": "^5.4.0",
    "gatsby-remark-smartypants": "^5.4.0",
    "gatsby-source-filesystem": "^4.4.0",
    "gatsby-transformer-json": "^4.4.0",
    "gatsby-transformer-remark": "^5.4.0",
    "gatsby-transformer-sharp": "^4.4.0",
    "jsonexport": "^3.2.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.25.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.22.5",
    "react-timeago": "^6.2.1",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.69.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/jsonexport": "^3.0.2",
    "@types/node": "^14.18.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@types/react-timeago": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "autoprefixer": "^10.3.4",
    "eslint": "^7.32.0",
    "gatsby-plugin-postcss": "^5.4.0",
    "jest": "^27.3.1",
    "postcss": "^8.3.6",
    "rimraf": "^3.0.2",
    "serverless": "^2.69.1",
    "serverless-s3-sync": "^1.17.3",
    "tailwindcss": "^3.0.7",
    "ts-jest": "^27.0.7"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  }
}
